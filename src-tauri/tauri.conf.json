{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "camfc-client",
  "version": "0.1.0",
  "identifier": "io.github.ant-cave",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "camfc-client",
        "width": 1152,
        "height": 648
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.ico"
    ]
  }
}
